# Set the Prezto modules to load (browse modules).
# The order matters.
zstyle ':prezto:load' starship \
  'autosuggestions'
  'syntax-highlighting'
  #'environment' \
  #'terminal' \
  #'editor' \
  #'history' \
  #'directory' \
  #'spectrum' \
  #'utility' \
  #'completion' \
  #'prompt' \
  #'git' \
  #'fasd' \
  #'rsync' \
  #'homebrew' \
  #'osx' \
  #'history-substring-search' \

zstyle ':prezto:module:editor' key-bindings 'vi'
